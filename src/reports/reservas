		<![CDATA[SELECT
     reserva."reser_motivo" AS Motivo,
     reserva."reser_data" AS Data,
     reserva."reser_horario_inicial" AS horario_inicial,
     reserva."reser_horario_final" AS horario_final,
     reserva."reser_confirmada" AS confirmada,
     reserva."sala_id" AS sala,
     usuario."usu_pnome" AS pnome,
     usuario."usu_unome" AS unome,
     usuario."usu_id" AS usuario_usu_id,
     reserva."reser_id" AS reserva_reser_id
FROM
     "public"."usuario" usuario INNER JOIN "public"."reserva" reserva ON usuario."usu_id" = reserva."usu_id"]]>
